{
  "name": "eslint-config-bridge5",
  "version": "1.5.0",
  "description": "Developer Tools for Project bridge5",
  "homepage": "https://eslint.bridge5.org",
  "repository": "git@github.com:bridge5/eslint-config-bridge5",
  "bugs": "https://github.com/bridge5/eslint-config-bridge5/issues",
  "prettier": "prettier-config-bridge5",
  "main": "index.js",
  "files": [
    ".eslintrc",
    "index.js",
    "legacy.js",
    "react.js",
    "lib"
  ],
  "keywords": [
    "javascript",
    "ecmascript",
    "eslint",
    "lint",
    "config",
    "prettier"
  ],
  "scripts": {
    "clean": "find . \\( -name 'node_modules' -or -name 'yarn.lock' -or -name 'yarn-error.log' \\) -prune -exec rm -rf '{}' +",
    "autod": "autod",
    "lint": "npx eslint test/*.test.js lib *.js",
    "test": "egg-bin test"
  },
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-eggache": "^1.0.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsdoc": "^30.7.11",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "autod": "^3.1.1",
    "coffee": "^5.4.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "egg-bin": "^4.15.0",
    "egg-ci": "^1.18.0",
    "eslint": "^7.17.0",
    "kill-port": "^1.6.1",
    "prettier": "^2.2.1",
    "prettier-config-bridge5": "^1.0.0",
    "prop-types": "^15.7.2",
    "rimraf": "^3.0.2"
  },
  "eslintIgnore": [
    "node_modules/*",
    "docs/*",
    "dist/*",
    "build/*",
    "coverage/*",
    "fixtures/*",
    "umd/*"
  ],
  "engines": {
    "node": ">=12.14.0",
    "yarn": ">=1.5.1"
  }
}
